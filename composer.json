{
    "name": "kallbuloso/laravel-blade-directives",
    "description": "My Blade directives",
    "keywords": [
        "kallbuloso",
        "laravel",
        "blade",
        "directives",
        "blade-directives"
    ],
    "homepage": "https://github.com/kallbuloso/laravel-blade-directives",
    "license": "MIT",
    "authors": [
        {
            "name": "Amaral karl",
            "email": "kallbuloso@kallbuloso.com.br",
            "homepage": "https://kallbuloso.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "kallbuloso\\BladeDirectives\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kallbuloso\\BladeDirectives\\Test\\": "tests",
            "kallbuloso\\BladeDirectives\\Test\\App\\": "tests/laravel/app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "kallbuloso\\BladeDirectives\\BladeDirectivesServiceProvider"
            ]
        }
    }
}
